.main(role='main')
    section.page-header
        .container
            .row
                .col-md-12
                    ul.breadcrumb
                        li
                            a(href='#') Home
                        li.active Portfolio
            .row
                .col-md-12
                    h1 Full Width
    .sort-source-wrapper
        .container
            ul.nav.nav-pills.sort-source.secondary.pull-right(data-sort-id='portfolio', data-option-key='filter')
                li.active(data-option-value='*')
                    a(href='#') Show All
                li(data-option-value='.websites')
                    a(href='#') Websites
                li(data-option-value='.logos')
                    a(href='#') Logos
                li(data-option-value='.brands')
                    a(href='#') Brands
